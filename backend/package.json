{
  "name": "nutrimap-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:reset": "prisma migrate reset --force",
    "seed": "ts-node src/seed.ts",
    "lint": "eslint . --ext .ts",
    "prepare": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node ./src/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@prisma/client": "^6.18.0",
    "@tanstack/react-query": "^5.90.5",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "lucide-react": "^0.548.0",
    "morgan": "^1.10.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "tailwindcss": "^4.1.16",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^24.9.1",
    "@types/supertest": "^2.0.16",
    "autoprefixer": "^10.4.21",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "prisma": "^6.18.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.6"
  }
}
